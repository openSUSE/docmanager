<!ENTITY stoponerrorentry "<varlistentry>
     <term><option>--stop-on-error</option></term>
     <listitem>
      <para>
       &stoponerrordesc;
      </para>
     </listitem>
    </varlistentry>">
<!ENTITY propertydesc "Property is the name of an XML tag.">